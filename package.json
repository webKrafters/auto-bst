    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^5.0.0",
    "jest-cli": "^26.6.3",
    "typescript": "^4.8.2"
  },
  "engines": {
    "node": ">=14.6.0"
  },
  "files": [
    "dist/index.js",
